<odoo noupdate="1">

    <record id="ir_cron_tms_gps_movildata" model="ir.cron">
        <field name="name">Download Movildata GPS</field>
        <field name="priority" eval="100" />
        <field name="interval_number">15</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field name="model">external.system.movildata</field>
        <field name="function">cron_import_gps_data</field>
        <field name="args">()</field>
    </record>

    <record id="external_system_movildata" model="external.system.movildata">
        <field name="name">GPS Movildata External System</field>
        <field name="system_type">external.system.movildata</field>
        <field name="host">https://ws.movildata.com/wsUsers.asmx?WSDL</field>
        <field name="company_ids" eval="[(5, 0), (4, ref('base.main_company'))]" />
    </record>

</odoo>
